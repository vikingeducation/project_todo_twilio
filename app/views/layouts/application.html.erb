<!DOCTYPE html>
<html>
<head>
  <title>Task Manager</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'shared/navbar' %>
  <% if flash.notice %>
  <div class="alert alert-success" role="alert">
    <div class="container">
      <%= flash.notice %>
    </div>
  </div>
  <% end %>
  <%= yield %>
</body>
</html>
