<%= render partial: 'navbar' %>
<h1>Tasks</h1>
<% @task.each do |task| %>
  <li>
    <%= link_to task.description, task_path(task) %>
    <%= link_to "Delete", task_path(task), method: :delete, data: {confirm: "Really delete the task?"}
    %>
    <%= link_to "Edit", edit_task_path(task) %>
  </li>
<% end %>